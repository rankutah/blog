  "blue-ridge-abbey": "npm run optimize:blue-ridge-abbey --silent && hugo server --disableFastRender --source ./sites/blue-ridge-abbey --gc --minify --logLevel debug --ignoreCache",
  "build:blue-ridge-abbey": "npm run optimize:blue-ridge-abbey --silent && hugo --source ./sites/blue-ridge-abbey --gc --minify --logLevel debug --ignoreCache",
  "quartz-worx": "npm run optimize:quartz-worx --silent && hugo server --disableFastRender --source ./sites/quartz-worx --gc --minify --logLevel debug --ignoreCache",
  "build:quartz-worx": "npm run optimize:quartz-worx --silent && hugo --source ./sites/quartz-worx --gc --minify --logLevel debug --ignoreCache",
  "optimize": "node scripts/optimize-images.mjs",
  "optimize:rank-utah": "node scripts/optimize-images.mjs --max-width=2000 --target-bytes=150000 --rewrite-refs=true sites/rank-utah/media",
  "optimize:nova-gutter": "node scripts/optimize-images.mjs --max-width=2000 --target-bytes=150000 --rewrite-refs=true sites/nova-gutter/media",
  "optimize:blue-ridge-abbey": "node scripts/optimize-images.mjs --max-width=2000 --target-bytes=150000 --rewrite-refs=true sites/blue-ridge-abbey/media",
