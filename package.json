  "type": "module",
  "main": "index.js",
  "scripts": {
  "rank-utah": "npm run optimize:rank-utah --silent && hugo server --disableFastRender --source ./sites/rank-utah --gc --minify --logLevel debug --ignoreCache",
  "build:rank-utah": "npm run optimize:rank-utah --silent && hugo --source ./sites/rank-utah --gc --minify --logLevel debug --ignoreCache",
  "nova-gutter": "npm run optimize:nova-gutter --silent && hugo server --disableFastRender --source ./sites/nova-gutter --gc --minify --logLevel debug --ignoreCache",
