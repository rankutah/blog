{{- /* Emits /.well-known/forms.json using [params.form] */ -}}
{{- $p := .Site.Params.form | default (dict) -}}
{
  "to": {{ (index $p "to" | default (slice)) | jsonify }},
  "from": {{ (index $p "from" | default "") | jsonify }},
  "subjectPrefix": {{ (index $p "subjectPrefix" | default "") | jsonify }},
  "allowedOrigins": {{ (index $p "allowedOrigins" | default (slice)) | jsonify }}
}
